    "src/js/**/*.{js,map}",
    "src/img/**/*.{svg}",
    "src/scss/**/*.scss"
  ],
  "style": "dist/css/tabler.css",
  "sass": "src/scss/tabler.scss",
  "unpkg": "dist/js/tabler.min.js",
  "umd:main": "dist/js/tabler.min.js",
  "module": "dist/js/tabler.esm.js",
  "main": "dist/js/tabler.js",
